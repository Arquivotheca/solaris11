$(MAN_INCLUDE)<include driver-network-ethernet-atge.man.p5m>
<include global_zone_only_component>
set name=pkg.fmri value=pkg:/driver/network/ethernet/atge@$(PKGVERS)
set name=pkg.description \
    value="Network device driver for Atheros (Attansic) L1E AR8121/AR8113 chipset-based Gigabit Ethernet NICs."
set name=pkg.summary value="Atheros (Attansic) L1E NIC driver"
set name=info.classification \
    value=org.opensolaris.category.2008:Drivers/Networking
set name=variant.arch value=i386
dir path=kernel group=sys
dir path=kernel/drv group=sys
dir path=kernel/drv/$(ARCH64) group=sys
driver name=atge \
    alias=pciex1969,1026 \
    alias=pciex1969,1048
file path=kernel/drv/$(ARCH64)/atge group=sys
legacy pkg=SUNWatge desc="Atheros/Attansic L1E Gigabit Ethernet" \
    name="Atheros/Attansic L1E Gigabit Ethernet 10/100/1000 Base (AR8121/AR8113)"
license cr_Sun license=cr_Sun
license lic_CDDL license=lic_CDDL
