depend fmri=desktop/xdg/xdg-utils type=require
depend fmri=pkg:/naming/ldap type=require
#
# kdcmgr(1m) uses /usr/sbin/ping
#
depend fmri=pkg:/network/ping type=require
depend fmri=pkg:/system/library/security/rpcsec type=require
