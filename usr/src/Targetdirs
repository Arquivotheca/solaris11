# /var/mail/:saved is built directly by the rootdirs target in
# /usr/src/Makefile because of the colon in its name.


# macros for symbolic links
SYM.DIRS= \
	/bin \
	/dev/stdin \
	/dev/stdout \
	/dev/stderr \
	/etc/log \
	/etc/svc/volatile \
	/etc/.syslog_door \
	/etc/syslog.pid \
	/sbin \
	/usr/adm \
	/usr/spool \
	/usr/lib/tmac \
	/usr/ccs/bin \
	/usr/preserve \
	/usr/lib/cron \
	/usr/mail \
	/usr/man \
	/usr/pub \
	/usr/src \
	/usr/tmp \
	/var/run

#
# Symlink assignments for 32/64 symlinks. There are a number of
# places where we have the following symlinks:
#	ln -s . 32
#	ln -s $(MACH64) 64
# BITSYMS is a list of these locations, used to generate the rules for
# the corresponding 32/64 symlinks at each location.
#

i386_BITSYMS= \
	usr/platform/i86pc/lib/fm/topo/plugins

sparc_BITSYMS= \
	usr/platform/sun4v/lib/fm/topo/plugins \
	usr/platform/sun4u/lib/fm/topo/plugins \
	usr/platform/SUNW,Sun-Fire/lib/fm/topo/plugins \
	usr/platform/SUNW,Sun-Fire-15000/lib/fm/topo/plugins \
	usr/platform/SUNW,SPARC-Enterprise/lib/fm/topo/plugins

BITSYMS= \
	$($(MACH)_BITSYMS) \
	lib \
	lib/crypto \
	lib/secure \
	usr/lib \
	usr/lib/brand/sn1 \
	usr/lib/brand/solaris10 \
	usr/lib/elfedit \
	usr/lib/libp \
	usr/lib/link_audit \
	usr/lib/lwp \
	usr/lib/secure \
	usr/lib/security \
	usr/ucblib \
	usr/xpg4/lib \
	usr/lib/fm/topo/plugins \
	var/ld

SYM.DIRS32= \
	$(BITSYMS:%=/%/32)

SYM.DIRS64= \
	$(BITSYMS:%=/%/64)

# prepend the ROOT prefix

ROOTDIRS=	$(TARGETDIRS:%=$(ROOT)%)
STUBROOTDIRS=	$(STUBTARGETDIRS:%=$(STUBROOT)%)

# conditional assignments
#
# Target directories with non-default values for owner and group must
# be referenced here, using their fully-prefixed names, and the non-
# default values assigned.  If a directory is mentioned above and not
# mentioned below, it has default values for attributes.
#
# The default value for DIRMODE is specified in usr/src/Makefile.master.
#

$(ROOT)/var/adm \
$(ROOT)/var/adm/sa :=		DIRMODE= 775

# file mode
#
$(ROOT)/tmp \
$(ROOT)/var/krb5/rcache \
$(ROOT)/var/preserve \
$(ROOT)/var/spool/pkg \
$(ROOT)/var/spool/uucppublic \
$(ROOT)/var/tmp:=	DIRMODE= 1777

$(ROOT)/root:=		DIRMODE= 700

$(ROOT)/var/krb5/rcache/root:=		DIRMODE= 700


#
# These permissions must match those set
# in the package manifests.
#
$(ROOT)/var/sadm/pkg \
$(ROOT)/var/sadm/security \
$(ROOT)/var/sadm/install/logs :=	DIRMODE= 555


#
# These permissions must match the ones set
# internally by fdfs and autofs.
#
$(ROOT)/dev/fd \
$(ROOT)/home:=		DIRMODE= 555

$(ROOT)/var/mail:=	DIRMODE=1777

$(ROOT)/proc:=		DIRMODE= 555

$(ROOT)/system/contract:=	DIRMODE= 555
$(ROOT)/system/object:=		DIRMODE= 555


#
# symlink assignments, LINKDEST is the value of the symlink
#
$(SYM.DIRS32:%=$(ROOT)/%)		$(SYM.DIRS32:%=$(STUBROOT)/%) := \
	LINKDEST=.
$(BUILD64) $(SYM.DIRS64:%=$(ROOT)/%)	$(SYM.DIRS64:%=$(STUBROOT)/%) := \
	LINKDEST=$(MACH64)
 
$(ROOT)/usr/lib/cron		$(STUBROOT)/usr/lib/cron := \
	LINKDEST=../../etc/cron.d
$(ROOT)/bin			$(STUBROOT)/bin := \
	LINKDEST=usr/bin
$(ROOT)/dev/stdin		$(STUBROOT)/dev/stdin := \
	LINKDEST=fd/0
$(ROOT)/dev/stdout		$(STUBROOT)/dev/stdout := \
	LINKDEST=fd/1
$(ROOT)/dev/stderr		$(STUBROOT)/dev/stderr := \
	LINKDEST=fd/2
$(ROOT)/usr/ccs/bin		$(STUBROOT)/usr/ccs/bin := \
	LINKDEST=../bin
$(ROOT)/sbin			$(STUBROOT)/sbin := \
	LINKDEST=usr/sbin
$(ROOT)/usr/pub			$(STUBROOT)/usr/pub := \
	LINKDEST=share/lib/pub
$(ROOT)/usr/man			$(STUBROOT)/usr/man := \
	LINKDEST=share/man
$(ROOT)/usr/src			$(STUBROOT)/usr/src := \
	LINKDEST=share/src
$(ROOT)/usr/adm			$(STUBROOT)/usr/adm := \
	LINKDEST=../var/adm
$(ROOT)/etc/log			$(STUBROOT)/etc/log := \
	LINKDEST=../var/adm/log
$(ROOT)/usr/mail		$(STUBROOT)/usr/mail := \
	LINKDEST=../var/mail
$(ROOT)/usr/preserve		$(STUBROOT)/usr/preserve := \
	LINKDEST=../var/preserve
$(ROOT)/usr/spool		$(STUBROOT)/usr/spool := \
	LINKDEST=../var/spool
$(ROOT)/usr/tmp			$(STUBROOT)/usr/tmp := \
	LINKDEST=../var/tmp
$(ROOT)/usr/lib/tmac		$(STUBROOT)/usr/lib/tmac := \
	LINKDEST=../share/lib/tmac
$(ROOT)/var/run			$(STUBROOT)/var/run := \
	LINKDEST=../system/volatile
$(ROOT)/etc/svc/volatile	$(STUBROOT)/etc/svc/volatile := \
	LINKDEST=../../system/volatile
$(ROOT)/etc/.syslog_door	$(STUBROOT)/etc/.syslog_door := \
	LINKDEST=../system/volatile/syslog_door
$(ROOT)/etc/syslog.pid		$(STUBROOT)/etc/syslog.pid := \
	LINKDEST=../system/volatile/syslog.pid



#
# Installing a directory symlink calls for overriding INS.dir to install
# a symlink.
#
$(DIRLINKS:%=$(ROOT)%)		$(DIRLINKS:%=$(STUBROOT)%) := \
	INS.dir= -$(RM) -r $@; $(SYMLINK) $(LINKDEST) $@

# Rather than adding another set of rules, we add usr/lib/lwp files here
$(ROOT)/usr/lib/libp/libc.so.1:=	REALPATH=../../../lib/libc.so.1
$(ROOT)/usr/lib/lwp/libthread.so.1:=	REALPATH=../libthread.so.1
$(ROOT)/usr/lib/lwp/libthread_db.so.1:=	REALPATH=../libthread_db.so.1

# symlinks to populate usr/lib/lwp/$(MACH64)
$(ROOT)/usr/lib/libp/$(MACH64)/libc.so.1:= \
	REALPATH=../../../../lib/$(MACH64)/libc.so.1
$(ROOT)/usr/lib/lwp/$(MACH64)/libthread.so.1:= \
	REALPATH=../../$(MACH64)/libthread.so.1
$(ROOT)/usr/lib/lwp/$(MACH64)/libthread_db.so.1:= \
	REALPATH=../../$(MACH64)/libthread_db.so.1

SYM.USRLWPLIB= \
	/usr/lib/libp/libc.so.1 \
	/usr/lib/lwp/libthread.so.1 \
	/usr/lib/lwp/libthread_db.so.1

SYM.USRLWPLIB64= \
	/usr/lib/libp/$(MACH64)/libc.so.1 \
	/usr/lib/lwp/$(MACH64)/libthread.so.1 \
	/usr/lib/lwp/$(MACH64)/libthread_db.so.1

