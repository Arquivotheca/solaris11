	/usr/lib/$(MACH64)/nss_user.so.1 \
	/usr/lib/fm/$(MACH64)/libfmevent.so \
	/usr/lib/fm/$(MACH64)/libfmevent.so.1 \
	/usr/lib/fm/$(MACH64)/llib-lfmevent.ln

#
# usr/src/Makefile uses INS.dir for any member of ROOTDIRS, the fact
# these are symlinks to files has no bearing on this.
#
$(FILELINKS:%=$(ROOT)%):= \
    INS.dir= -$(RM) $@; $(SYMLINK) $(REALPATH) $@


#
# List of objects from the build system to receive symbolic links in
# the stub proto. These objects are available for linking even when
# ZASSERTDEFLIB is in use, without requiring an explicit whitelisting
# via ZASSERTDEFLIB_LIST.
#
# Objects that always have a known fixed location in the build system
# should be on this list. Objects that have a dynamic location, such
# as those supplied by the compilers, should go on ZASSERTDEFLIB_LIST.
#
SYM.BLDSYSLIB= \
	/lib/libcrypto.so \
	/lib/libm.so \
	/lib/libssl.so \
	/lib/libxml2.so \
	/lib/libz.so \
	/usr/lib/libcurl.so \
	/usr/lib/libdbus-1.so \
	/usr/lib/libdbus-glib-1.so \
	/usr/lib/libexpat.so \
	/usr/lib/libglib-2.0.so \
	/usr/lib/libgnomevfs-2.so \
	/usr/lib/libgobject-2.0.so \
	/usr/lib/libnetsnmp.so \
	/usr/lib/libnetsnmpagent.so \
	/usr/lib/libnetsnmphelpers.so \
	/usr/lib/libpython2.6.so \
	/usr/lib/libtspi.so \
	/usr/lib/libwsreg.so \
	/usr/lib/libxslt.so

sparcv9_SYM.BLDSYSLIB64= \
	/lib/sparcv9/libmtsk.so

SYM.BLDSYSLIB64= \
	$($(MACH64)_SYM.BLDSYSLIB64) \
	/lib/$(MACH64)/libcrypto.so \
	/lib/$(MACH64)/libm.so \
	/lib/$(MACH64)/libssl.so \
	/lib/$(MACH64)/libxml2.so \
	/lib/$(MACH64)/libz.so \
	/usr/lib/$(MACH64)/libcurl.so \
	/usr/lib/$(MACH64)/libdbus-1.so \
	/usr/lib/$(MACH64)/libdbus-glib-1.so\
	/usr/lib/$(MACH64)/libexpat.so \
	/usr/lib/$(MACH64)/libglib-2.0.so \
	/usr/lib/$(MACH64)/libgobject-2.0.so \
	/usr/lib/$(MACH64)/libnetsnmp.so \
	/usr/lib/$(MACH64)/libnetsnmpagent.so \
	/usr/lib/$(MACH64)/libnetsnmphelpers.so \
	/usr/lib/$(MACH64)/libpython2.6.so \
	/usr/lib/$(MACH64)/libtspi.so \
	/usr/lib/$(MACH64)/libxslt.so

#
# As with FILELINKS, we overload INS.dir to deliver the stub proto
# symlinks that point at objects we link to from the build system.
#
$(BLDSYSLINKS:%=$(STUBROOT)%):= \
    INS.dir= -$(RM) $@; $(SYMLINK) $(@:$(STUBROOT)%=%) $@
