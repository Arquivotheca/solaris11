SYMBOL_VERSION SUNW_1.4 {
    global:
	freeaddrinfo;
	gai_strerror;
	getaddrinfo;
	getnameinfo;
	if_freenameindex;
	if_indextoname;
	if_nameindex;
	if_nametoindex;
