$mapfile_version 2

STUB_OBJECT;

SYMBOL_VERSION SUNW_1.7 {
    global:
	freeifaddrs;
	getifaddrs;
} SUNW_1.6;

SYMBOL_VERSION SUNW_1.6 {
    global:
	getipv4sourcefilter;
	getsourcefilter;
	setipv4sourcefilter;
	setsourcefilter;
} SUNW_1.5;

