include $(SRC)/Makefile.master

SUBDIRS = \
	allow	\
	authtok_get	\
	authtok_check	\
	authtok_store	\
	deny		\
	dhkeys		\
	dial_auth	\
	krb5		\
	krb5_migrate	\
	ldap		\
	list		\
	passwd_auth	\
	rhosts_auth	\
	roles		\
	sample		\
	smb		\
	smbfs		\
	tsol_acct	\
	tty_tickets	\
	unix_auth	\
	unix_account	\
	unix_cred	\
	unix_session	\
	zfs_key

all :=		TARGET= all
catalog :=	TARGET= _msg
clean :=	TARGET= clean
clobber :=	TARGET= clobber
install :=	TARGET= install
lint :=		TARGET= lint
_msg :=		TARGET= _msg

# PAM modules are not linked to via the link-editor, and stubs are not needed.
# Provide NULL make rules to satisfy the parent makefiles.
stub:
stubinstall:

.KEEP_STATE:

all clean clobber install lint _msg: $(SUBDIRS)

$(SUBDIRS): FRC
	@cd $@; pwd; $(MAKE) $(TARGET)

FRC:
