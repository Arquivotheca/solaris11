$mapfile_version 2

CAPABILITY sun4v {
	MACHINE = sun4v;
	HW += SHA256;
	HW += SHA512;
	HW += MD5;
	HW += SHA1;
};

SYMBOL_SCOPE {
	global:
		MD5Init;
                MD5Update;
                MD5Final;
		SHA1Init;
		SHA1Update;
		SHA1Final;
		SHA2Init;
		SHA2Update;
		SHA2Final;
		SHA256Init;
		SHA256Update;
		SHA256Final;
		SHA384Init;
		SHA384Update;
		SHA384Final;
		SHA512Init;
		SHA512Update;
		SHA512Final;
        local:
                *;
};
