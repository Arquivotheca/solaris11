		gid		CDATA #REQUIRED
		ruid		CDATA #REQUIRED
		rgid		CDATA #REQUIRED
		pid		CDATA #REQUIRED
		sid		CDATA #REQUIRED
		tid		CDATA #REQUIRED
>

