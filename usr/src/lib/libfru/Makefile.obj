LIBRARYCCC=	libfru.a
VERS=		.1

SED=		sed

MAINOBJS=	Parser.o \
		PayloadReader.o \
		libfru.o \
		Ancestor.o

YACCOBJS=	nameSyntaxYacc.o
LEXOBJS=	nameSyntaxLex.o

GENUTOBJS=	Str.o

OBJECTS=	$(MAINOBJS) \
		$(YACCOBJS) \
		$(LEXOBJS) \
		$(GENUTOBJS)

MAINSRCS=	$(MAINOBJS:%.o=./libfru/%.cc)
YACCSRCS=	$(YACCOBJS:%.o=./libfru/%.y)
LEXSRCS=	$(LEXOBJS:%.o=./libfru/%.l)
GENUTSRCS=	$(GENUTOBJS:%.o=./libgenutil/%.cc)

SRCS=		$(MAINSRCS) \
		$(YACCSRCS) \
		$(LEXSRCS) \
		$(GENUTSRCS)

# include common Makefiles
include ../../Makefile.lib

LIBS=		$(DYNLIBCCC)
STUBLIBS=	$(DYNLIBCCC)

MAPFILEDIR = ../libfru
