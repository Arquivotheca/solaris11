include ../../../Makefile.master

HDRS=	\
	doserror.h		\
	lmerr.h			\
	mailslot.h		\
	nmpipes.h		\
	ntdsutil.h		\
	nterror.h		\
	ntlmssp.h		\
	ntifs.h			\
	ntstatus.h		\
	smb.h			\
	smb_sid.h		\
	spnego.h		\
	wintypes.h

ROOTDIR= $(ROOT)/usr/include/smb
ROOTHDRS= $(HDRS:%=$(ROOTDIR)/%)
CHECKHDRS= $(HDRS:%.h=%.check)

$(ROOTDIR)/%: %
	$(INS.file)

$(ROOTDIR):
	$(INS.dir)

.KEEP_STATE:

.PARALLEL: $(CHECKHDRS)

install_h: $(ROOTDIR) $(ROOTHDRS)

check: $(CHECKHDRS)
