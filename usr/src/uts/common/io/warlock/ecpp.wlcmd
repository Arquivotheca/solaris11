root ecpp_open
root ecpp_close 
root ecpp_wput
root ecpp_wsrv
root ecpp_rsrv
root ecpp_kstat_update
root ecpp_kstatintr_update

add	bus_ops::bus_add_eventcall targets warlock_dummy
