	mdb_v9util.c

REGOFFUSERS = \
	kaif_resume.o \
	kaif_startup.o

CTXOFFUSERS = \
	kmdb_setcontext.o

$(REGOFFUSERS) $(REGOFFUSERS:%.o=%.ln): kaif_off.h

$(CTXOFFUSERS) $(CTXOFFUSERS:%.o=%.ln): kmdb_context_off.h

