ASFLAGS += -I$(ROOT)/usr/include/v9
CPPFLAGS += -I$(ROOT)/usr/include/v9

PROMINCDIRS += $(SRC)/uts/sun4u

KMDBML += \
	client_handler.s \
	hcall.s \
	kaif_handlers.s \
	kaif_invoke.s \
	kaif_resume.s \
	kaif_startup.s \
	kmdb_setcontext.s \
	kmdb_start.s \
	kmdb_v9asmutil.s

