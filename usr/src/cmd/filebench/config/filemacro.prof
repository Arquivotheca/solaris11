DEFAULTS {
	runtime = 120;
        dir = /tmp;
        stats = /tmp;
	filesystem = tmpfs;
	description = "filemacro tmpfs";
}

CONFIG fileserver {
	function = generic;
	personality = fileserver;
	nfiles = 20000;
	meandirwidth = 20;
	filesize = 2k;
	nthreads = 100;
	meaniosize = 16k;
}

CONFIG varmail {
	personality = varmail;
	function = generic;
	nfiles = 20000;
	meandirwidth = 1000000;
	filesize = 1k;
	nthreads = 16;
	meaniosize = 16k;
}

CONFIG webproxy {
	personality = webproxy;
