#include <link.h>
#include <dlfcn.h>

void *	dlopen(const char *, int);
void *	dlsym(void *_RESTRICT_KYWD, const char *_RESTRICT_KYWD);
int	dlclose(void *);
char *	dlerror(void);
void *	dlmopen(Lmid_t, const char *, int);
