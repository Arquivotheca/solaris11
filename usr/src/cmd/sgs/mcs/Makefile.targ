%.o:		../common/%.c
		$(COMPILE.c) $<
		$(POST_PROCESS_O)

%.o:		$(SGSTOOLS)/common/%.c
		$(COMPILE.c) $<
		$(POST_PROCESS_O)

all:		$(PROG) $(STRIPFILE)

clean:
		$(RM) $(CLEANFILES)

delete:
		$(RM) $(PROG)

lint:		$(LINTOUT32) $(SGSLINTOUT)

$(PROG):	$(OBJS)
		$(LINK.c) -o $@ $(OBJS) $(LDLIBS)
		$(POST_PROCESS)

$(STRIPFILE):

$(ROOTLINKS):	$(VAR_SGSBINPROG)
		$(RM) $@
		$(LN) $(VAR_SGSBINPROG) $@

$(ROOTBIN64)/%: %
		$(INS.file)

include		$(SRC)/cmd/Makefile.targ
include		$(SRC)/cmd/sgs/Makefile.targ
