	    PROP Label string r 0 "PCI0"
	ENDNODE
	NODE PCI1 location
	    PROP SlotType string r 0 "pci"
	    PROP Label string r 0 "PCI1"
	ENDNODE
	NODE PCI2 location
	    PROP SlotType string r 0 "pci"
	    PROP Label string r 0 "PCI2"
	ENDNODE
	NODE PCI3 location
