# cmd/krb5/Makefile
#
# Build everything in parallel; use .WAIT for dependencies
.PARALLEL:

SUBDIRS= \
	kwarn	.WAIT	\
	kdc \
	kadmin	\
	kdestroy	\
	kinit	\
	klist \
	kproplog \
	krb5-config \
	kvno \
	slave \
	ldap_util

all :=		TARGET= all
clean :=	TARGET= clean
clobber :=	TARGET= clobber
delete :=	TARGET= delete
install :=	TARGET= install
lint :=		TARGET= lint
catalog :=	TARGET= catalog
package :=	TARGET= package
_msg :=		TARGET= _msg

.KEEP_STATE:

all clean clobber delete install lint catalog package _msg: $(SUBDIRS)

check: $(CHECKHDRS)

# ldap_util has a dependency on kadmin/cli/getdate.o
ldap_util: kadmin

$(SUBDIRS):	FRC
	@cd $@; pwd; $(MAKE) $(TARGET)

FRC:
