#
# Common definitions for all smbsrv commands
#

OBJS=	$(SRCS:%.c=%.o)

LINTFLAGS += -xerroff=E_NAME_DEF_NOT_USED2 -xerroff=E_NAME_USED_NOT_DEF2

CPPFLAGS += $(INCS)
CFLAGS += $(CCVERBOSE)
$(NOT_RELEASE_BUILD)CPPFLAGS += -DDEBUG

C99MODE =       -xc99=%all
C99LMODE =      -Xc99=%all
